{
  "background": {
    "scripts": ["./background.js"]
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["./contentScripts.js"],
      "matches": [
        "http://*.missevan.com/*",
        "http://*.bilibili.com/*",
        "http://*.bilibili.co/*",
        "https://*.missevan.com/*",
        "http://*.bilibili.co/*",
        "https://*.bilibili.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "missevan activity devtools",
  "icons": {
    "16": "assets/icons/icon.png",
    "32": "assets/icons/icon.png",
    "64": "assets/icons/icon.png",
    "128": "assets/icons/icon.png"
  },
  "manifest_version": 2,
  "name": "missevan-activity-devtools",
  "permissions": [
    "http://*.missevan.com/*",
    "http://*.bilibili.com/*",
    "http://*.bilibili.co/*",
    "https://*.missevan.com/*",
    "http://*.bilibili.com/*",
    "https://*.bilibili.co/*",
    "clipboardWrite"
  ],
  "version": "1.1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
